{
  "name": "rubic-catalog",
  "version": "1.0.0",
  "private": true,
  "description": "Rubic official catalog (VSCode extension version)",
  "main": "lib/update.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-schema": "download https://github.com/kimushu/rubic-vscode/raw/master/schema/catalog.d.ts > src/catalog.d.ts",
    "compile": "tsc -watch -p ./",
    "install-compile": "npm install && npm run compile",
    "force-pull": "git fetch origin && git reset --hard origin/vscode-master",
    "main": "node lib/update.js",
    "prepare": "npm run force-pull && npm install",
    "auto-update": "npm run prepare && npm run main && npm run auto-commit",
    "auto-commit": "git diff --quiet catalog.json || (git commit catalog.json -m \"Automatic catalog update\" && git push)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kimushu/rubic-catalog.git"
  },
  "keywords": [
    "Rubic"
  ],
  "author": "kimu_shu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kimushu/rubic-catalog/issues"
  },
  "homepage": "https://github.com/kimushu/rubic-catalog#readme",
  "devDependencies": {
    "@types/node": "^7.0.11",
    "download-cli": "^1.0.1",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "decompress": "^4.0.0",
    "deep-equal": "^1.0.1",
    "github": "^9.2.0",
    "pify": "^2.3.0",
    "request": "^2.81.0",
    "uuid": "^3.0.1"
  }
}
